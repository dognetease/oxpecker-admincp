{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "baseUrl": "./",
        "moduleResolution": "node",
        "paths": {
            "@/*": ["src/*"]
        },
        "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
    },
    "resolveExtensions": [".js", ".vue", ".ts"],
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
    "typeAcquisition": {
        "include": ["lodash"]
    }
}
